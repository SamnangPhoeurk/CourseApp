<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.thoeunchhaykry.shortcauseapp.HomeActivity"
    tools:ignore="MissingConstraints">

    <ScrollView
        android:layout_width="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        android:layout_height="match_parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="UselessParent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <RelativeLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:ignore="ScrollViewSize">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/first_block"
                tools:ignore="DisableBaselineAlignment">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    android:layout_weight="0.5"
                    android:layout_marginRight="1dp"
                    android:background="@color/network"
                    android:layout_marginBottom="1dp"
                    tools:ignore="RtlHardcoded"
                    android:id="@+id/network_layout">

                    <ImageView
                        android:layout_width="60dp"
                        android:layout_height="match_parent"
                        app:srcCompat="@drawable/network"
                        android:id="@+id/imageView2"
                        tools:ignore="ContentDescription"
                        android:layout_marginLeft="8dp" />

                    <TextView
                        android:text="Network"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/textView7"
                        android:fontFamily="sans-serif-condensed"
                        android:textStyle="normal|bold"
                        android:textSize="18sp"
                        android:textColor="@android:color/white"
                        android:textAlignment="textStart"
                        android:gravity="center_vertical"
                        android:layout_marginLeft="10dp"
                        tools:ignore="HardcodedText,RtlHardcoded" />

                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    android:layout_weight="0.5"
                    android:layout_marginLeft="1dp"
                    android:background="@color/network"
                    android:layout_marginBottom="1dp"
                    tools:ignore="RtlHardcoded"
                    android:id="@+id/design_layout">

                    <ImageView
                        android:layout_width="68dp"
                        android:layout_height="match_parent"
                        app:srcCompat="@drawable/graphic"
                        android:id="@+id/imageView4"
                        android:layout_marginLeft="8dp"
                        tools:ignore="ContentDescription" />

                    <TextView
                        android:text="Design"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/textView9"
                        android:layout_marginLeft="10dp"
                        android:gravity="center_vertical"
                        android:fontFamily="sans-serif-condensed"
                        android:textSize="18sp"
                        android:textColor="@android:color/white"
                        tools:ignore="HardcodedText"
                        android:textStyle="normal|bold" />

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/second_block"
                android:layout_below="@+id/first_block"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                tools:ignore="DisableBaselineAlignment">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:layout_weight="0.5"
                    android:layout_marginRight="1dp"
                    android:background="@color/program"
                    android:layout_marginTop="1dp"
                    tools:ignore="RtlHardcoded"
                    android:layout_marginBottom="1dp"
                    android:paddingBottom="1dp"
                    android:weightSum="1"
                    android:id="@+id/program_layout">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="79dp"
                        app:srcCompat="@drawable/program"
                        android:id="@+id/imageView5"
                        android:layout_weight="0.56"
                        tools:ignore="ContentDescription,NestedWeights" />

                    <TextView
                        android:text="Programming"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView11"
                        android:layout_weight="0.14"
                        android:textSize="18sp"
                        android:fontFamily="sans-serif-condensed"
                        android:textAlignment="center"
                        android:gravity="center_vertical"
                        android:layout_marginTop="-15dp"
                        android:textColor="@android:color/white"
                        tools:ignore="HardcodedText"
                        android:textStyle="normal|bold" />

                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:layout_weight="0.5"
                    android:layout_marginBottom="1dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="1dp"
                    tools:ignore="RtlHardcoded"
                    android:id="@+id/multimedia_layout">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="90dp"
                        android:layout_weight="0.5"
                        android:background="@color/law"
                        android:layout_marginBottom="1dp"
                        tools:ignore="NestedWeights,RtlHardcoded">

                        <ImageView
                            android:layout_width="69dp"
                            android:layout_height="wrap_content"
                            app:srcCompat="@drawable/multimedai"
                            android:id="@+id/imageView6"
                            android:layout_marginLeft="8dp"
                            tools:ignore="ContentDescription" />

                        <TextView
                            android:text="Multimedia"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/textView12"
                            android:fontFamily="sans-serif-condensed"
                            android:textSize="18sp"
                            android:textColor="@android:color/white"
                            android:textStyle="normal|bold"
                            android:textAlignment="textStart"
                            android:gravity="center_vertical"
                            android:layout_marginLeft="8dp"
                            tools:ignore="HardcodedText" />

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="90dp"
                        android:layout_weight="0.5"
                        android:background="@color/law"
                        android:layout_marginTop="1dp"
                        tools:ignore="RtlHardcoded"
                        android:id="@+id/repair_layout">

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="55dp"
                            app:srcCompat="@drawable/repair"
                            android:id="@+id/imageView7"
                            tools:ignore="ContentDescription"
                            android:layout_marginTop="4dp" />

                        <TextView
                            android:text="Computer Repair"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/textView13"
                            android:textAlignment="center"
                            android:textStyle="normal|bold"
                            android:fontFamily="sans-serif-condensed"
                            android:textSize="18sp"
                            android:layout_marginLeft="8dp"
                            android:gravity="fill_vertical"
                            android:textColor="@android:color/white"
                            tools:ignore="HardcodedText" />

                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:background="@color/civil"
                tools:ignore="RtlHardcoded"
                android:layout_below="@+id/second_block"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="1dp"
                android:layout_marginBottom="1dp"
                android:id="@+id/three_block">

                <ImageView
                    android:layout_width="161dp"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/micsoftoffice"
                    android:id="@+id/imageView8"
                    android:padding="10dp"
                    tools:ignore="ContentDescription" />

                <TextView
                    android:text="Microsoft Office "
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/textView14"
                    android:fontFamily="sans-serif-condensed"
                    android:textSize="18sp"
                    android:textColor="@android:color/white"
                    android:textStyle="normal|bold"
                    android:gravity="center_vertical"
                    tools:ignore="HardcodedText"
                    android:layout_marginLeft="8dp" />

            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/four_block"
                android:layout_below="@+id/three_block"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                tools:ignore="DisableBaselineAlignment"
                android:layout_marginTop="1dp"
                android:layout_marginBottom="1dp">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:layout_weight="0.5"
                    tools:ignore="RtlHardcoded"
                    android:layout_marginRight="1dp">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="90dp"
                        android:layout_weight="0.5"
                        android:background="@color/eng"
                        android:layout_marginBottom="1dp"
                        tools:ignore="NestedWeights,RtlHardcoded"
                        android:id="@+id/towDthreeD_layout">

                        <ImageView
                            android:layout_width="59dp"
                            android:layout_height="match_parent"
                            app:srcCompat="@drawable/towdandthreed"
                            android:id="@+id/imageView"
                            android:layout_marginLeft="8dp"
                            tools:ignore="ContentDescription" />

                        <TextView
                            android:text="2D and 3D"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:id="@+id/textView"
                            android:layout_weight="1"
                            android:layout_marginLeft="8dp"
                            android:fontFamily="sans-serif-condensed"
                            android:textSize="18sp"
                            android:textColor="@android:color/white"
                            android:textStyle="normal|bold"
                            android:textAlignment="textStart"
                            android:gravity="center_vertical"
                            tools:ignore="HardcodedText,InefficientWeight" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="90dp"
                        android:layout_weight="0.5"
                        android:background="@color/eng"
                        android:layout_marginTop="1dp"
                        tools:ignore="RtlHardcoded"
                        android:id="@+id/business_layout">

                        <ImageView
                            android:layout_width="72dp"
                            android:layout_height="match_parent"
                            app:srcCompat="@drawable/business"
                            android:id="@+id/imageView9"
                            android:layout_marginLeft="8dp"
                            tools:ignore="ContentDescription" />

                        <TextView
                            android:text="Business"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:id="@+id/textView4"
                            android:layout_weight="0.85"
                            android:textStyle="normal|bold"
                            android:textSize="18sp"
                            android:fontFamily="sans-serif-condensed"
                            android:textColor="@android:color/white"
                            android:gravity="center_vertical"
                            android:layout_marginLeft="8dp"
                            tools:ignore="HardcodedText,InefficientWeight" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:layout_weight="0.5"
                    android:background="@color/divider"
                    tools:ignore="RtlHardcoded"
                    android:paddingBottom="1dp"
                    android:layout_marginLeft="1dp"
                    android:id="@+id/langluage_layout">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="98dp"
                        app:srcCompat="@drawable/langlaugeicon"
                        android:id="@+id/imageView10"
                        android:layout_marginTop="15dp"
                        tools:ignore="ContentDescription" />

                    <TextView
                        android:text="Langluage"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView2"
                        android:textSize="18sp"
                        android:fontFamily="sans-serif-condensed"
                        android:textStyle="normal|bold"
                        android:textAlignment="center"
                        android:textColor="@android:color/white"
                        android:layout_marginTop="10dp"
                        tools:ignore="HardcodedText" />
                </LinearLayout>


            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/five_block"
                tools:ignore="DisableBaselineAlignment"
                android:layout_below="@+id/four_block"
                android:layout_marginTop="1dp"
                android:layout_marginBottom="1dp">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    android:layout_weight="0.5"
                    android:layout_marginRight="1dp"
                    android:background="@color/eco"
                    tools:ignore="RtlHardcoded"
                    android:id="@+id/economic_layout">

                    <ImageView
                        android:layout_width="60dp"
                        android:layout_height="match_parent"
                        app:srcCompat="@drawable/economic"
                        android:id="@+id/imageView3"
                        tools:ignore="ContentDescription"
                        android:layout_marginLeft="8dp" />

                    <TextView
                        android:text="Economic"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/textView33"
                        android:fontFamily="sans-serif-condensed"
                        android:textStyle="normal|bold"
                        android:textSize="18sp"
                        android:textColor="@android:color/white"
                        android:textAlignment="textStart"
                        android:gravity="center_vertical"
                        android:layout_marginLeft="10dp"
                        tools:ignore="HardcodedText,RtlHardcoded" />

                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    android:layout_weight="0.5"
                    android:layout_marginLeft="1dp"
                    android:background="@color/eco"
                    tools:ignore="RtlHardcoded"
                    android:id="@+id/law_layout">

                    <ImageView
                        android:layout_width="78dp"
                        android:layout_height="match_parent"
                        app:srcCompat="@drawable/law"
                        android:id="@+id/imageView11"
                        android:layout_marginLeft="8dp"
                        tools:ignore="ContentDescription" />

                    <TextView
                        android:text="Law"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:id="@+id/textView5"
                        android:layout_weight="0.91"
                        android:textStyle="normal|bold"
                        android:textAlignment="textStart"
                        android:textSize="18sp"
                        android:fontFamily="sans-serif-condensed"
                        android:textColor="@android:color/white"
                        android:layout_marginLeft="8dp"
                        android:gravity="center_vertical"
                        tools:ignore="HardcodedText,InefficientWeight,NestedWeights" />
                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>
